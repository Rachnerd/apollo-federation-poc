{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@generated/*": ["./.generated/*"]
    },
    "lib": []
  },
  "include": ["./**/*.ts"]
}
